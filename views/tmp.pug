form(action="/remove?prospect="+index method="POST" style="float: right;")
  input.btn.btn-danger.rmv(
    type='submit'
    aria-hidden="true"
    value="remove"
  )


  form(action="/remove?index="+index method="POST" style="float: right;")
    input.btn.btn-danger.rmv(
      type='submit'
      aria-hidden="true"
      value="remove"
      style="margin-right: 0.5rem;"
    )
    
    
    form(action="/remove?inter="+index method="POST" style="float: right;")
      input.btn.btn-danger.rmv(
        type='submit'
        aria-hidden="true"
        value="remove"
        style=""
      )
      
      form(action="/remove?reject="+index method="POST" style="float: right;")
        input.btn.btn-danger.rmv(
          type='submit'
          aria-hidden="true"
          value="remove"
          style="margin-right: 0!important;"
        )
